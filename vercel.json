{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.8"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/manage.py"
    }
  ]
}
