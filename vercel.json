{
  "version": 2,
  "builds": [
    {
      "src": "todo_app/wsgi.py",  // Adjust the path to `wsgi.py` based on your project structure
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "todo_app/wsgi.py"  // Make sure this path points to your `wsgi.py` file
    }
  ]
}
